
#include <iostream>
#include <stdlib.h>
#include <vector>

using namespace std;

int main()
{
	int n;
	vector<int> a;
	static int count;

	cin >> n;

	for (int i = 0; i < n + 1; i++)
	{
		a.push_back(rand() % 10);
	}

	for (int k = 0; k < a.size(); k++)
	{
		cout << a[k] << " ";
	}

	cout << endl;

	int find;

	cin >> find;

	for (int j = 0; j < n + 1; j++)
	{
		if (a[j] == find)
		{
			count += 1;
		}
	}
	cout << count;
}

